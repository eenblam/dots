(defn chooser [[fc sc & oc]]
  (println (str "first: " fc))
  (println (str "second: " sc))
  (println (str "Others: " (clojure.string/join ", " oc))))
(chooser '('a' 'b' 'c'))
(chooser (list a b c))
(chooser (list 'a' 'b' 'c'))
(chooser (list "a" "b" "c"))
(chooser ["a", "b", "c"])
(chooser ["a" "b" "c"])
(rand-int 3)
(doc take)
(doc some)
(doc repeatedly)
(take 10 (repeatedly rand))
(use 'clojure.math.numeric-tower)
(use 'clojure.contrib.math)
